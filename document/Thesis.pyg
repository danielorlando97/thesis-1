from search_space import FunctionalConstraint

class MySeachSpace:
    ADomain = Domain[int](max = 100)
    def __init__(
        self,
        a: int = Domain[int] | (
            lambda x, y = ADomain: (IsEven(y) | x == 10 )
        ),
        b: float Domain[float] | (
            lambda x, y = ADomain: (IsEven(y) & x % 1 == 0 )
        )
    )
        self.a, self.b = a, b

